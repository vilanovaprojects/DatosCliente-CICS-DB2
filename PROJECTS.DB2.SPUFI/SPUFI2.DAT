-- SPUFI CREATE TABLE                                                   
     CREATE TABLE USRDATOS                                              
        (                                                               
          CIF              CHAR(9)   NOT NULL                           
        , NOMBRE           CHAR(20)  NOT NULL                           
        , DIRECCION        CHAR(35)  NOT NULL                           
        , TLF              CHAR(9)   NOT NULL                           
        , CORREO           CHAR(20)  NOT NULL                           
                                                                        
        , PRIMARY KEY (CIF)                                             
        )                                                               
                                                                        
        IN VILANDB.DATOSTS;                                             
                                                                        
                                                                        
      CREATE UNIQUE INDEX USRDATOS_IPK ON USRDATOS                      
        (                                                               
          CIF                                                           
        )                                                               
         CLUSTER CLOSE NO                                               
         USING STOGROUP VILANSG                                         
         BUFFERPOOL BP2;                                                
                                                                        
